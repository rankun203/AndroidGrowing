<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.mindfine.simpleuitest">

    <uses-sdk android:minSdkVersion="7" android:targetSdkVersion="16"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <activity android:label="@string/app_name" android:name="com.mindfine.simpleuitest.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_display_message" android:name="com.mindfine.simpleuitest.DisplayMessageActivity" android:parentActivityName="com.mindfine.simpleuitest.MainActivity"/>
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="com.mindfine.simpleuitest.MainActivity"/>

    </application>

</manifest>
